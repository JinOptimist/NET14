@model List<FilesViewModel>
@{
    Layout = "/Views/Shared/_LayoutSocial.cshtml";
}
<div class="main-content">
    <div>
        <a href="/Social/YourFiles">Назад</a>
    </div>
    <div class="zagolovok">
        <div>
            Смотри какие есть авто
        </div>
        <div>
            @foreach (var Files in Model)
            {
                <div>   
                    <a href="/Social/ShowImage?id=@Files.Id">@Files.Name</a>
                </div>
            }
        </div>
    </div>
</div>

