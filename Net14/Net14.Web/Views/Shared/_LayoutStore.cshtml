    </div>
    <div class="container">
        @RenderBody()
    </div>
    <footer class="pagefooter">

        <div class="leftfooter">
            <div>
                <div>© 2022 TMSNET14 </div>
                <div>Artem,Mark,Anton,Andrey </div>
            </div>
            <div class="adress">
                г.Минск ул.Тимирязева 67 оф.601
            </div>
            <div class="iconfoot">
            </div>
        </div>

    </footer>


