@model List<UserViewModel>
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<link rel="stylesheet" href="~/css/messanger.css" />

<div class="all-messanger">
    <div class="other-people">
        @foreach (UserViewModel user in Model)
       {
           <div class="user">
               @user.FirstName @user.LastName . @user.EnglishLevel
           </div>
       }
    </div>
    <div class="messanger">
        <input type="text" />
        <input type="submit" />
    </div>
</div>

